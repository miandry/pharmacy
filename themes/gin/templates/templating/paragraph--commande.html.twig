{% set el = paragraph_parser(paragraph) %}
{% set article = node_parser(el.field_article.nid) %}
<div class="row details-order">
  <div class="col-md-3 title-order">
    {{ article.title }}
  </div>
  <div class="col-md-3 prix_unitaire">
    {{ el.field_prix_unitaire }}
  </div>
  <div class="col-md-3 quantity">
    {{ el.field_quantite }}
  </div>
  <div class="col-md-3 total_price">
    {{ el.field_quantite * el.field_prix_unitaire }}
  </div>
</div>
