<form>
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Date start</label>
    <input type="text" class="form-control" name="date_start" id="exampleInputEmail1" aria-describedby="emailHelp">
  </div>
    <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Date end</label>
    <input type="text" class="form-control" name="date_end" id="exampleInputEmail1" aria-describedby="emailHelp">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>

<table data-toggle="table">
	<thead>
		<tr>
			<th>Item ID</th>
			<th>Nom article</th>
			<th>Total quantite vendu</th>
            <th>Nombre achat article </th>
		</tr>
	</thead>
	<tbody>
        {% for item in items %}
          <tr>
            <td>{{item.article_nid}}</td>
			<td>{{item.title}}</td>
			<td>{{item.total_quantity}}</td>
            <td>{{item.repeat_count}}</td>
		  </tr>

    {% endfor %}
	
	</tbody>
</table>

